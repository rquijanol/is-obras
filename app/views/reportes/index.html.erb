<h1>Listing reportes</h1>

<table>
  <thead>
    <tr>
      <th>Clima</th>
      <th>Fecha</th>
      <th>Incidencias</th>
      <th>Cambios</th>
      <th>Residente</th>
      <th>Supervisor</th>
      <th>Obra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reportes.each do |reporte| %>
      <tr>
        <td><%= reporte.clima %></td>
        <td><%= reporte.fecha %></td>
        <td><%= reporte.incidencias %></td>
        <td><%= reporte.cambios %></td>
        <td><%= reporte.residente %></td>
        <td><%= reporte.supervisor %></td>
        <td><%= reporte.obra %></td>
        <td><%= link_to 'Show', reporte %></td>
        <td><%= link_to 'Edit', edit_reporte_path(reporte) %></td>
        <td><%= link_to 'Destroy', reporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reporte', new_reporte_path %>
