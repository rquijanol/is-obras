<%= form_for (@reporte) do |f| %>
  <% if @reporte.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@reporte.errors.count, "error") %> prohibited this reporte from being saved:</h2>

      <ul>
      <% @reporte.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :clima %><br>
    <%= f.text_field :clima %>
  </div>
  <div class="field">
    <%= f.label :fecha %><br>
    <%= f.date_select :fecha %>
  </div>
  <div class="field">
    <%= f.label :incidencias %><br>
    <%= f.text_area :incidencias %>
  </div>
  <div class="field">
    <%= f.label :cambios %><br>
    <%= f.text_area :cambios %>
  </div>
  <div class="field">
    <%= f.label :residente %><br>
    <%= f.text_field :residente %>
  </div>
  <div class="field">
    <%= f.label :supervisor %><br>
    <%= f.text_field :supervisor %>
  </div>
  <div class="field">
    <%= f.label @reporte.obra.nombre %>
  </div>
  <%= f.hidden_field :obra_id, { :value => @reporte.obra.id } %>
  
  
 <div id="detalles">
  <h3>Tasks</h3>
  <% f.fields_for :reporte_detalle do |reporte_detalles_form| -%>
   <%= render :partial => 'reporte_detalles/reporte_detalles', :locals => { :f => reporte_detalles_form } %>
  <% end -%>
</div> 
  
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
